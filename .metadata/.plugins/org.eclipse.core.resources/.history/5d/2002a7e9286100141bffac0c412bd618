public class FractionCalculatorTest{
	//tests input
	@Test
	public void inputtest(){
	String myString = "3/4 + 1/-3 * 7 / 5";
	
	String  splitString[] = myString.split("\\s+");
	int i = 1;
	for (String element : splitString) {
        System.out.println(element);
        System.out.println("i = " + i);
        i++;
    }
	}
}
